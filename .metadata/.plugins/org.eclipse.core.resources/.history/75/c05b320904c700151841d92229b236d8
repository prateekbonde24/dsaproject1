#include"team_stats.h"
using namespace std;
string name;
vector <int> globalArr;
bool sortFlag = false;
int t_compare= 0;
int no_swaps=0;
int arrLength=0;
int num_of_years;
void start()
{
	cout << name <<"********" << endl;

}

void readFile()
{
	globalArr.clear();
	arrLength = 0;

	string filename = name+".txt";
	ifstream myfile (filename.c_str());
	if (myfile.is_open())
	{vector <string> arr;
		int c=0;
		string line;
		while( getline(myfile, line)){
			if (!line.empty())
					{
						if(c==0){
							stringstream ss(line);
					    ss>>num_of_years;
					    cout<<num_of_years;

						}

						if(c!=0){
							if (!line.empty())
												{
												    arr.push_back(line);
									        	    c++;
												}
									         }

						}

						c++;
					}
		         }

	else {
			cout << "-------------------Can't open file----------------------"<<endl;
		}
			    myfile.close();
		}





void end()
{
	cout<<"End of processing fitness data for: "+name <<endl;
	name.clear();
	globalArr.clear();
	sortFlag = false;
	arrLength = 0;
}

void exitFunc()
{
	cout<<"Program terminating"<<endl;
	exit(0);
}

int main() {
	for (string line; getline(cin, line);) {

			   name = line;




		   }
	readFile();

	return 0;
}


