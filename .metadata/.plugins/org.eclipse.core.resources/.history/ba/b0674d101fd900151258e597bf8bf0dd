#include <iostream>
using namespace std;

int main() {
#include<ctime>
#include<iostream>
#include<cstdlib>
#include<fstream>

using namespace std;

int main()
{
	int n;
	cerr << "How many random numbers do you want to generate?\n";
	cin >> n;
    cout << n << endl;

	srand( (unsigned) time(NULL) );

	for(int i = 0; i < n; i++){
		cout << rand() % 100000 << endl;
	}
}
}
